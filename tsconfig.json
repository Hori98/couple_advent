{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "types": [
      "nativewind/types"
    ]
  },
  "include": [
    "app",
    "components",
    "hooks",
    "lib",
    "constants",
    "types",
    "tailwind.config.js",
    "nativewind.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
